<layout version="1.0">
    <catalog_category_layered>
        <reference name="head">
            <action method="addItem">
                <type>skin_js</type>
                <name>js/jquery-1.11.1.min.js</name>
            </action>
            <action method="addItem">
                <type>skin_js</type>
                <name>js/noConflict.js</name>
            </action>
            <action method="addItem">
                <type>skin_js</type>
                <name>js/filter/jquery-ui.js</name>
            </action>

            <action method="addItem">
                <type>skin_js</type>
                <name>js/filter/filter.js</name>
            </action>

            <action method="addItem">
                <type>skin_css</type>
                <name>css/filter/jquery-ui.css</name>
            </action>
        </reference>

        <reference name="left">
            <remove name="catalog.leftnav"/>
            <block type="teamto_filter/layer_view" name="filter" after="currency" template="teamto/filter/filter.phtml"/>
        </reference>
    </catalog_category_layered>
</layout>