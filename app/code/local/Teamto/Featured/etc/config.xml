<?xml version="1.0"?>
<config>
    <modules>
        <Teamto_Featured>
            <version>0.1.0</version>
        </Teamto_Featured>
    </modules>
    <global>
        <models>
            <teamto_featured>
                <class>Teamto_Featured_Model</class>
                <resourceModel>teamto_featured_resource</resourceModel>
            </teamto_featured>

            <teamto_featured_resource>
                <class>Teamto_Featured_Model_Resource</class>
            </teamto_featured_resource>
        </models>

        <blocks>
            <featured>
                <class>Teamto_Featured_Block</class>
            </featured>
        </blocks>

        <helpers>
            <featured>
                <class>Teamto_Featured_Helper</class>
            </featured>
        </helpers>

        <resources>
            <teamto_featured_setup_ver4>
                <setup>
                    <module>Teamto_Featured</module>
                    <class>Teamto_Featured_Model_Resource_Setup</class>
                </setup>
            </teamto_featured_setup_ver4>
        </resources>
    </global>

    <frontend>
        <routers>
            <featured>
                <use>standard</use>
                <args>
                    <module>Teamto_Featured</module>
                    <frontName>featured</frontName>
                </args>
            </featured>
        </routers>

        <layout>
            <updates>
                <featured>
                    <file>featured.xml</file>
                </featured>
            </updates>
        </layout>
    </frontend>


</config>